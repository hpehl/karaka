<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:tire="urn:import:name.pehl.tire.client.activity.view">

    <g:HTMLPanel>
        <tire:WeekChartWidget ui:field="weekChart" width="200" height="200"
            legendTitles="Mo, Tue, Wed, Thu, Fr, Sat, Sun" prevTooltip="Previous calendarweek" titleTooltip="Current Calendarweek"
            nextTooltip="Next calendarweek" />
        <tire:MonthChartWidget ui:field="monthChart" width="200" height="200" legendTitles="CW?, CW?, CW?, CW?"
            prevTooltip="Previous month" titleTooltip="Current month" nextTooltip="Next month" />
    </g:HTMLPanel>

</ui:UiBinder> 